#include <SoftwareSerial.h>

SoftwareSerial mySerial(0, 1); // RX, TX

int photoResistor1 = A1;
int photoResistor2 = A2;
int photoResistor3 = A3;
int photoResistor4 = A4;
int photoResistor5 = A5;

void setup() {
  Serial.begin(9600);
  mySerial.begin(9600);
}

void loop() {
  int val1 = analogRead(photoResistor1);
  int val2 = analogRead(photoResistor2);
  int val3 = analogRead(photoResistor3);
  int val4 = analogRead(photoResistor4);
  int val5 = analogRead(photoResistor5);

  if(val1 > 500) {
    mySerial.println("A1");
    Serial.println("A1");
  }
  if(val2 > 600) {
    mySerial.println("A2");
    Serial.println("A2");
  }
  if(val3 > 600) {
    mySerial.println("A3");
    Serial.println("A3");
  }
  if(val4 > 1000) {
    mySerial.println("A4");
    Serial.println("A4");
  }
  if(val5 > 600) {
    mySerial.println("A5");
    Serial.println("A5");
  }

  delay(1000);
}
